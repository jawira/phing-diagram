{
  "name": "jawira/phing-visualizer",
  "type": "library",
  "description": "Graphical representation of Phing's buildfile",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Jawira Portugal"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-simplexml": "*",
    "ext-xsl": "*",
    "jawira/mini-getopt": "^0.0.1",
    "jawira/plantuml-encoding": "^1.0"
  },
  "require-dev": {
    "behat/behat": "^3.5",
    "kamermans/command": "^1.2",
    "mikey179/vfsStream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Jawira\\PhingVisualizer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jawira\\PhingVisualizer\\Behat\\": "tests/behat/bootstrap"
    }
  },
  "bin": [
    "bin/phing-visualizer"
  ]
}
